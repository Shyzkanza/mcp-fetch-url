{
  "name": "@shyzus/mcp-scrapidou",
  "version": "1.1.0",
  "description": "Scrapidou - MCP server for web scraping and URL fetching",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "mcp-scrapidou": "dist/http-client.js"
  },
  "files": [
    "dist/http-client.js",
    "README.md"
  ],
  "keywords": [
    "mcp",
    "scraping",
    "fetch",
    "web",
    "url",
    "html",
    "metadata",
    "chatgpt"
  ],
  "author": "Rankorr",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  },
  "volta": {
    "node": "20.19.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Shyzkanza/mcp-fetch-url"
  },
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "rebuild": "npm run clean && npm run build",
    "dev": "npm run kill --silent; nvm use 20 > /dev/null 2>&1 || true; tsx watch src/http-server.ts",
    "dev:http": "npm run dev",
    "dev:tunnel": "npm run kill:tunnel --silent; nvm use 20 > /dev/null 2>&1 || true; npm-run-all --parallel dev tunnel",
    "start": "npm run kill --silent; nvm use 20 > /dev/null 2>&1 || true; node dist/http-server.js",
    "start:http": "npm run start",
    "build:start": "npm run build && npm run start",
    "kill": "lsof -ti:${PORT:-3000} | xargs kill -9 2>/dev/null || true",
    "kill:tunnel": "pkill -f ngrok 2>/dev/null || true",
    "tunnel": "ngrok http ${PORT:-3000}",
    "inspect": "npx @modelcontextprotocol/inspector@latest http://localhost:${PORT:-3000}/mcp",
    "health": "curl -s http://localhost:${PORT:-3000}/health | jq .",
    "prepare": "npm run build"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.23.0",
    "@mozilla/readability": "^0.6.0",
    "cheerio": "^1.1.0",
    "jsdom": "^22.1.0"
  },
  "devDependencies": {
    "@types/jsdom": "^27.0.0",
    "@types/node": "^20.10.0",
    "npm-run-all": "^4.1.5",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  }
}
